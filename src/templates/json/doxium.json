{
    "$schema": "https://raw.githubusercontent.com/konyogony/doxium/main/src/templates/json/schema.json",
    "style": {
        "base-color": "$COLOR",
        "accent-color": "$ACCENT-COLOR",
        "shiki-theme": "$SHIKI-THEME",
    },
    "use-docs": $USEDOCS,
    "alias": {
        "components": "$COMPONENTS-ALIAS",
        "lib": "$LIB-ALIAS",
        "types": "$TYPES-ALIAS",
    },
    "base-url": "$BASE-URL",
    "socials": {
        "github-repo": "$GITHUB-REPO",
        "twitter": "",
        "discord": ""
    },
    "misc": {
        "separate": false,
    }
}